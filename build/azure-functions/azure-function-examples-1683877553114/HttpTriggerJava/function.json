{
  "scriptFile" : "../azure-function-examples-1.0.0-SNAPSHOT.jar",
  "entryPoint" : "com.eurocontrol.spring.functions.HttpTriggerHandler.run",
  "bindings" : [ {
    "type" : "httpTrigger",
    "direction" : "in",
    "name" : "req",
    "methods" : [ "POST" ],
    "authLevel" : "FUNCTION"
  }, {
    "type" : "http",
    "direction" : "out",
    "name" : "$return"
  } ]
}